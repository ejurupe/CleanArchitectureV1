@model IEnumerable<GalaxyTraining.Sales.Application.Customers.Queries.GetCustomerList.CustomerModel>

@{
    ViewBag.Title = "Clientes";
}

<h2>Customers</h2>
<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var customer in Model)
    {
        <tr>
            <td>@customer.Id</td>
            <td>@customer.Name</td>
        </tr>
    }
    </tbody>
</table>

